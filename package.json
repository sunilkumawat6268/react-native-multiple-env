{
  "name": "my-app",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "setDevelopment": "ENVFILE=.env.dev",
    "setProduction": "ENVFILE=.env.prod",
    "run:aDev": "yarn setDevelopment react-native run-android --mode=devDebug --appIdSuffix=dev",
    "run:aProd": "yarn setProduction react-native run-android --mode=prodDebug",
    "run:iProd": "react-native run-ios --scheme 'myapp'",
    "run:iDev": "react-native run-ios --scheme 'myappDev'"
  },
  "dependencies": {
    "expo": "~51.0.28",
    "expo-status-bar": "~1.12.1",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-config": "^1.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "typescript": "^5.1.3"
  },
  "private": true
}
